<%= link_to 'New Working Card', new_working_card_path %>

<%= link_to 'New Coaster', new_coaster_path %>

<h2> Coaster </h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3">Quantity</th>
    </tr>
  </thead>

  <tbody>
    <% @coasters.try(:each) do |coaster| %>
      <tr>
        <td><%= coaster.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2> Staff Card </h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3">Quantity</th>
    </tr>
  </thead>

  <tbody>
    <% @working_cards.try(:each) do |working_card| %>
      <tr>
        <td><%= working_card.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
