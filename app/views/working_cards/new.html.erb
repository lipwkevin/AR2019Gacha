<h1>New Working Card</h1>
<%= form_tag(working_cards_path) do %>
  <div class="field">
    <%= label_tag 'name' %>
    <%= select("post", "character", @characters.to_a, {}) %>
  </div>
  <div class="field">
    <%= label_tag 'quantity' %>
    <%= select("post", "quantity", (1..10).to_a, {}) %>
  </div>
  <div class="actions">
    <%= submit_tag  %>
  </div>
<% end %>

<%= link_to 'Back', backend_path %>
