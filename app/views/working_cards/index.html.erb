<p id="notice"><%= notice %></p>

<h1>Working Cards</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @working_cards.each do |working_card| %>
      <tr>
        <td><%= working_card.name %></td>
        <td><%= link_to 'Show', working_card %></td>
        <td><%= link_to 'Edit', edit_working_card_path(working_card) %></td>
        <td><%= link_to 'Destroy', working_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Working Card', new_working_card_path %>
